{
  "routes": [
    {
      "src": "^/sitemap.xml$",
      "dest": "/sitemap.xml",
      "headers": {
        "Content-Type": "text/xml; charset=utf-8",
        "Cache-Control": "no-cache, no-store, must-revalidate",
        "Pragma": "no-cache",
        "Expires": "0"
      }
    },
    {
      "src": "^/((?!sitemap\\.xml).*)$",
      "dest": "/index.html"
    }
  ]
}
